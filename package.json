{
  "name": "to-nice-number",
  "version": "0.0.2",
  "description": "A JavaScript implementation of \"Nice Numbers for Graph Labels\" from Graphic Gems",
  "homepage": "https://github.com/fisker/to-nice-number#readme",
  "bugs": {
    "url": "https://github.com/fisker/to-nice-number/issues"
  },
  "repository": "fisker/to-nice-number",
  "funding": "https://github.com/fisker/to-nice-number?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./dist/index.min.mjs",
    "require": "./dist/index.min.cjs"
  },
  "main": "./dist/index.min.cjs",
  "jsdelivr": "./dist/index.min.js",
  "unpkg": "./dist/index.min.js",
  "module": "./dist/index.min.mjs",
  "browser": "./dist/index.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean build lint test dist",
    "test": "ava",
    "test-coverage": "c8 yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "verbose": true
  },
  "c8": {
    "include": [
      "src/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.8",
    "@commitlint/cli": "16.0.2",
    "@fisker/commitlint-config": "2.0.2",
    "@fisker/eslint-config": "10.0.10",
    "@fisker/eslint-config-ava": "2.0.4",
    "@fisker/husky-config": "4.1.1",
    "@fisker/lint-staged-config": "3.1.4",
    "@fisker/prettier-config": "5.0.4",
    "@rollup/plugin-babel": "5.3.0",
    "ava": "4.0.1",
    "c8": "7.11.0",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "4.0.1",
    "eslint": "8.7.0",
    "fast-cartesian-product": "2.0.1",
    "husky": "7.0.4",
    "lint-staged": "12.2.1",
    "markdownlint-cli": "0.30.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "rollup": "2.64.0",
    "rollup-plugin-prettier": "2.2.2",
    "rollup-plugin-terser": "7.0.2",
    "sort-package-json": "1.53.1",
    "table": "6.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
